numero1= int(input('digite um numero inteiro:'))
numero2= int(input('digite outro numero inteiro:'))
numero3= int(input('digite outro numero inteiro:'))
numero4= int(input('digite outro numero inteiro:'))
numero5= int(input('digite outro numero inteiro:'))
numero6= int(input('digite outro numero inteiro:'))
numero7= int(input('digite outro numero inteiro:'))
numero8= int(input('digite outro numero inteiro:'))
numero9= int(input('digite outro numero inteiro:'))
numero10= int(input('digite outro numero inteiro:'))

total=(numero1,numero2,numero3,numero4,numero5,numero6,numero7,numero8, numero9,numero10)
print(total)
for i in total:
   if i %2 ==0:
      print('numero par',i)
   else: i % 2 !=0
   print('numero impar',i)
